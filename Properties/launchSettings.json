{
    "$schema": "http://json.schemastore.org/launchsettings.json",
    "iisSettings": {
        "windowsAuthentication": false,
        "anonymousAuthentication": true,
        "iisExpress": {
            "applicationUrl": "http://localhost:63448",
            "sslPort": 44312
        }
    },
    "profiles": {
        "http": {
            "commandName": "Project",
            "dotnetRunMessages": true,
            "launchBrowser": false, // Não abrir o navegador automaticamente
            "applicationUrl": "http://localhost:5212", // URL da aplicação
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        "https": {
            "commandName": "Project",
            "dotnetRunMessages": true,
            "launchBrowser": false, // Não abrir o navegador automaticamente
            "applicationUrl": "https://localhost:7093", // URL da aplicação com HTTPS
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        "IIS Express": {
            "commandName": "IISExpress",
            "launchBrowser": false, // Não abrir o navegador automaticamente
            "applicationUrl": "http://localhost:5212", // URL da aplicação no IIS Express
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    }
}